DAY 8
======
MAERGE TWO SORTED ARRAY WITHOUT EXTRA SPACE 
+==========================================
class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
       int j=m-1;
       int i=n-1,k=nums1.length-1;
       while(i>=0)
       {
           if(j>=0 && nums1[j]>nums2[i])
           {
               nums1[k]=nums1[j];
               k--;
               j--;
           }

           else
           {
               nums1[k]=nums2[i];
               k--;
               i--;
           }
       }
    }
}
